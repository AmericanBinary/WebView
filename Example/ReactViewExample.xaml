<Window x:Class="Example.ReactViewExample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Example"
        mc:Ignorable="d"
        Title="ReactViewExample" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Bottom" Click="OnWPFButtonClick">WPF button</Button>
        <Button DockPanel.Dock="Bottom" Click="OnShowDevTools">Show dev tools</Button>

        <local:ExampleView x:Name="exampleView" Click="OnExampleViewClick" GetTime="OnExampleViewGetTime" ConstantMessage="This is an example" Image="Beach" />
    </DockPanel>
</Window>
